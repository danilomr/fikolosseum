{
	"all": true,
	"check-coverage": true,
	"report-dir": "coverage",
	"reporter": ["json", "lcov"],
	"include": ["src"],
	"exclude": [
		"**/*.d.ts",
		"**/*.scss",
		"**/*{.,-}test.{ts,tsx,js}",
		"**/index.ts",
		"src/accessibility.ts",
		"src/components/ErrorBoundary",
		"src/components/Localize",
		"src/Welcome"
	],
	"watermarks": {
		"lines": [80, 95],
		"functions": [80, 95],
		"branches": [80, 95],
		"statements": [80, 95]
	},
	"per-file": true
}
